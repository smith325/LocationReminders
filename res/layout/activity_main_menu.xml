<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout
	android:id="@+id/widget0"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android">
<Button
	android:id="@+id/NewListButton"
	android:layout_width="122dp"
	android:layout_height="wrap_content"
	android:background="@drawable/button_style" 
	android:text="New List"
	android:layout_x="90dp"
	android:layout_y="124dp" />
<Button
	android:id="@+id/LoadListButton"
	android:layout_width="122dp"
	android:layout_height="wrap_content"
	android:background="@drawable/button_style" 
	android:text="Load List"
	android:layout_x="91dp"
	android:layout_y="191dp" />
<Button
	android:id="@+id/ShareListButton"
	android:layout_width="122dp"
	android:layout_height="wrap_content"
	android:background="@drawable/button_style" 
	android:text="Share List"
	android:layout_x="91dp"
	android:layout_y="260dp" />

<Button
    android:id="@+id/ViewListButton"
    android:layout_width="116dp"
    android:layout_height="wrap_content"
    android:background="@drawable/button_style" 
    android:layout_x="96dp"
    android:layout_y="323dp"
    android:text="View List" />

<Button
    android:id="@+id/ClearAllButton"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/button_style" 
    android:layout_x="104dp"
    android:layout_y="385dp"
    android:text="Clear All" />

</AbsoluteLayout>

